{
  "name": "svelte-app-template",
  "version": "0.8.0",
  "description": "Svelte app template",
  "main": "rollup.config.js",
  "author": "ink8bit",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ink8bit/svelte-app-template.git"
  },
  "bugs": {
    "url": "https://github.com/ink8bit/svelte-app-template/issues"
  },
  "homepage": "https://github.com/ink8bit/svelte-app-template#readme",
  "private": true,
  "scripts": {
    "clean": "rm -rf build",
    "prebuild": "npm run clean",
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "check": "svelte-check",
    "type:check": "tsc --noEmit",
    "lint": "eslint --ext .js,.ts",
    "format:check": "prettier --check \"src/**/*\"",
    "format": "prettier --write \"src/**/*\"",
    "prepare": "node ./prepare-hooks.js"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-html": "^0.2.4",
    "@rollup/plugin-image": "^2.1.1",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-typescript": "^8.3.0",
    "@tsconfig/svelte": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.34.0",
    "eslint": "^8.8.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.4",
    "prettier": "^2.5.1",
    "prettier-plugin-svelte": "^2.6.0",
    "rollup": "2.67",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-serve": "^1.1.0",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "svelte": "^3.46.4",
    "svelte-check": "^2.4.3",
    "svelte-preprocess": "^4.10.3",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5"
  }
}
